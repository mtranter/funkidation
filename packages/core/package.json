    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "standard-version": "^9.1.0",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.20.5",
    "typescript": "^4.1.3"
  }
}
